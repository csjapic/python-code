#Problem 1
import pandas as pd
import numpy as np

def sum_of_multiples(limit):
    total_sum = 0
    for number in range(limit):
        if number % 3 == 0 or number % 5 ==0:
            total_sum += number
    return total_sum
    
result = sum_of_multiples(1000)
print(result)

